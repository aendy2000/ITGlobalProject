                                    else
                                    {
                                        <li> <a class="nav-link " href="@Url.Action("Index","DuAnThanhCong")">DỰ ÁN THÀNH CÔNG</a> </li>

                                    }
                                    @if (ViewBag.HeaderPages == "HoiDap")
                                    {
                                        <li> <a class="nav-link active" href="@Url.Action("Index","HoiDap")">HỎI ĐÁP</a> </li>
