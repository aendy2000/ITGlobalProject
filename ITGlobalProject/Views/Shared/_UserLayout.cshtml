    <div class="loader" id="AjaxLoader" style="display:none;"></div>
    <div id="dtr-wrapper" class="clearfix">
        <div class="dtr-responsive-header fixed-top" style="">
            <div class="container">

                <a href="@Url.Action("Index","TrangChu")">
                    <img src="~/Content/User/public/main/img/icon.svg" style="width: 20%" alt="logo">
                </a>

                <button id="dtr-menu-button" class="dtr-hamburger" type="button">
                    <span class="dtr-hamburger-lines-wrapper">
                        <span class="dtr-hamburger-lines"></span>
                    </span>
                </button>
            </div>
            <div class="dtr-responsive-header-menu">
                <div class="slicknav_menu">
                    <ul class="slicknav_nav slicknav_hidden dtr-scrollspy" style="touch-action: pan-y; display: none;" aria-hidden="true" role="menu">
