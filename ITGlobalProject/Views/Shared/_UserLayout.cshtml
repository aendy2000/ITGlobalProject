                                            <li><a href="#tab3">Tư vấn - thiết kế phần mềm</a></li>
                                        </ul>
                                    </div>
                                    <div class="col-12 col-lg-4 col-md-12">
                                        <ul class="dtr-list-border">
                                            <li><a href="@Url.Action("Index","DuAnThanhCong")" target="_blank">Dự án</a></li>
                                            <li><a href="@Url.Action("Index","HoiDap")" target="_blank">Hỏi đáp</a></li>
                                            <li><a href="@Url.Action("Index","LienHe")" target="_blank">Liên hệ</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <!-- nested row ends -->

                            </div>
                            <!-- column 2 ends -->
                            <!-- column 3 starts -->
