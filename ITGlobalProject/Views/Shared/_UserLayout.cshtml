                                    style="touch-action: pan-y;">
                                    @if (ViewBag.HeaderPages == "TrangChu")
                                    {
                                        <li> <a class="nav-link active" href="@Url.Action("Index","TrangChu")">TRANG CHỦ</a> </li>
                                    }
