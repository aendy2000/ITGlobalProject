                            <label style="font-weight:bold" for="assign" class="col-form-label-sm">Người thực hiện  <span class="text-danger">*</span></label>
                            <input class="form-control  form-control-sm" list="assign" placeholder="Chọn người thực hiện" value="1 - Đặng Văn Tuấn - Web Development" />
                            <datalist data-width="100%" id="assign">
                                <option value="1 - Đặng Văn Tuấn - Web Development">
                                <option value="2 - Lê Thị Lợi - Tester">
                                <option value="3 - Nguyễn Hiếu Nhân - Web Development">
