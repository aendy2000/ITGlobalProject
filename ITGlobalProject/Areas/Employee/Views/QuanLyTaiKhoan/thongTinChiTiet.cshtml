                        buttons: {
                            cancel: {
                                visible: true,
                                text: ' Hủy Bỏ ',
                                className: 'btn btn-danger'
                            },
                            confirm: {
                                text: 'Xác Nhận',
                                className: 'btn btn-success'
                            }
                        }
                    }).then((roikhoitrang) => {
                        if (roikhoitrang) {
                            somethingChanged = false;
                            $('#AjaxLoader').fadeIn('slow');
                            $.ajax({
                                url: '@Url.Action("doiMatKhauPartial", "quanlytaikhoan", new {id = Model.ID })',
