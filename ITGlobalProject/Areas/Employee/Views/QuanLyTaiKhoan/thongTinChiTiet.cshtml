                                    $('#AjaxLoader').fadeOut('slow');
                                }
                                else {
                                    $('#AjaxLoader').fadeOut('slow');
                                    window.location.href = $('#actionDanhsach').data('request-url');
                                }
                            });

                        }
                    });
                };
                return {
                    init: function () {
                        initDemos();
                    },
                };
