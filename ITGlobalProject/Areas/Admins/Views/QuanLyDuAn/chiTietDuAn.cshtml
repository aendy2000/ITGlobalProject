<script>
    $(document).on('click', '#tongQuanID', function (e) {
        $('#AjaxLoader').show();
    $.ajax({
        url: '@Url.Action("tongQuanPartial", "QuanLyDuAn", new {id = Model.ID})',
        type: 'GET',
        dataType: 'html'
    }).done(function(ketqua) {
        if (ketqua == "DANHSACH" || ketqua == "DANGNHAP") {
            $('#AjaxLoader').hide();
            window.location.href = '@Url.Action("danhSachDuAn", "QuanLyDuAn")';
        } else {
            $('#chiTietDuAnPartialID').replaceWith(ketqua);
            $.when(
                $.getScript("@Url.Content("~/Content/Admin/assets/js/plugin/sweetalert/sweetalert.min.js")"),
                $.getScript("@Url.Content("~/Content/Admin/assets/js/theme.min.js")"),
                $.getScript("@Url.Content("~/Content/Admin/assets/libs/flatpickr/dist/flatpickr.min.js")"),
                $.getScript("@Url.Content("~/Content/Admin/assets/libs/apexcharts/dist/apexcharts.min.js")"),
                $.Deferred(function(deferred) {
                    $(deferred.resolve);
                })
            ).done(function() {

            });
            $('#AjaxLoader').hide();
        }
    });
});

    $(document).on('click', '#congViecID', function (e) {
        $('#AjaxLoader').show();
    $.ajax({
        url: '@Url.Action("congViecPartial", "QuanLyDuAn", new {id = Model.ID })',
        type: 'GET',
        dataType: 'html'
    }).done(function(ketqua) {
