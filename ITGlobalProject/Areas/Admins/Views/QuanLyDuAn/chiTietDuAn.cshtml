<div class="col-md-12 mb-4">
    <!-- card -->
    <div class="card">
        <div class="card-header">
            <h4 class="mb-0">Thông Tin Khách Hàng </h4>

        </div>
        <!-- Card body -->
        <div class="card-body">
            <div class="d-flex align-items-center">
                <div class="position-relative">
                    @if (string.IsNullOrEmpty(Model.Partners.Avatar))
                    {
                        <img src="~/Content/Admin/assets/images/avatar/default-avatar.png" alt="" class="rounded-circle avatar-xl" />
                    }
                    else
                    {
                        <img src="@Model.Partners.Avatar" alt="" class="rounded-circle avatar-xl" />
                    }
                </div>
                <div class="ms-4">
                    <h3 class="mb-0">@Model.Partners.Name</h3>
                    <p class="mb-0 fs-5">@Model.Partners.Company</p>
                    <p class="mb-1 fs-5">@Model.Partners.Email</p>
                </div>
            </div>
            <div style="text-align: center; margin-bottom: -20px" class="border-top row mt-3">
                <div class="col">
                    <div class="pe-1 ps-2 py-3">
                        <h5 class="mb-0">@Model.Partners.Phone</h5>
                        <span class="fs-6">Số điện thoại</span>
                    </div>
                </div>
                <div class="col border-start">
                    <div class="pe-1 ps-3 py-3">
                        <h5 class="mb-0">@Model.Partners.Projects.Count.ToString("D2")</h5>
                        <span class="fs-6">Dự án đang có</span>
                    </div>
                </div>
                <div class="col border-start">
                    <div class="pe-1 ps-3 py-3">
                        <a href="#" style="width: 90%; margin-top:5px" role="button" class="btn btn-outline-primary btn-sm">Xem chi tiết</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col-md-12 mb-4">
    <!-- card -->
    <div class="card">
        <!-- card body -->

        <div class="card-header">
            <h4 class="mb-0">
                Tiến Độ Dự Án
            </h4>

        </div>
        <!-- table -->
        <div class="table-responsive overflow-y-hidden">
            <table class="table mb-0 text-nowrap">
                <thead class="table-light">
                    <tr>
                        <th scope="col" class="border-top-0">Thành viên</th>
                        <th scope="col" class="border-top-0 ">Vai trò</th>
                        <th scope="col" class="border-top-0 ">Số việc </th>
                        <th scope="col" class="border-top-0 ">Hoàn thành</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Teams.Count() > 0)
                    {
                        foreach (var item in Model.Teams)
                        {
                            <tr>
                                <td class="align-middle">
                                    <div class="d-flex align-items-center">
                                        <div class="avatar avatar-sm avatar-danger">
                                            @if (string.IsNullOrEmpty(item.Employees.Avatar))
                                            {
                                                <span class="avatar-initials rounded-circle "> @item.Employees.Name.Substring(0, 1) </span>
                                            }
                                            else
                                            {
                                                <img src="@item.Employees.Avatar" alt="" class="avatar-initials rounded-circle">
                                            }
                                        </div>
                                        <div class="ms-2">
                                            <h5 class="mb-0">@item.Employees.Name </h5>
                                        </div>
                                    </div>
                                </td>
                                <td class="align-middle ">
                                    @item.Employees.Position.Name
                                </td>
                                <td class="align-middle ">
                                    Còn @Model.Tasks.Where(t => t.ID_Employee == item.ID_Employee && !t.State.Equals("done")).ToList().Count/@Model.Tasks.Where(t => t.ID_Employee == item.ID_Employee).ToList().Count
                                </td>
                                <td class="align-middle ">
                                    @if (Model.Tasks.Where(t => t.ID_Employee == item.ID_Employee).ToList().Count < 1)
                                    {
                                        <div class="d-flex align-items-center">
                                            <div class="progress flex-auto" style="height: 6px;">
                                                <div class="progress-bar bg-success " role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">

                                                </div>
                                            </div>
                                            <div class="ms-2"> <span>0% </span> </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="d-flex align-items-center">
                                            <div class="progress flex-auto" style="height: 6px;">
                                                <div class="progress-bar bg-success " role="progressbar" style="width: @(Convert.ToDecimal((Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee && t.State.Equals("done")).Sum(t => t.OriginalEstimate) / Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee).Sum(t => t.OriginalEstimate)) * 100).ToString("0"))%;" aria-valuenow="@(Convert.ToDecimal((Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee && t.State.Equals("done")).Sum(t => t.OriginalEstimate) / Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee).Sum(t => t.OriginalEstimate)) * 100).ToString("0"))" aria-valuemin="0" aria-valuemax="100">

                                                </div>
                                            </div>
                                            <div class="ms-2"> <span>@(Convert.ToDecimal((Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee && t.State.Equals("done")).Sum(t => t.OriginalEstimate) / Model.Tasks.Where(t=> t.ID_Employee == item.ID_Employee).Sum(t => t.OriginalEstimate)) * 100).ToString("0"))% </span> </div>
                                        </div>
                                    }
                                </td>
                            </tr>

                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="4" class="text-center md-4">
                                Không có dữ liệu để hiển thị!
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
</div>
</div>

</div>
<div class="col-md-12 col-xl-4 col-12">
    <!-- card  -->
    <div class="card mb-4 bg-primary border-primary">
        <!-- card body  -->
        <div class="card-body">

            <h3 class="card-title text-white">Thời Gian Còn Lại </h3>

            <!-- dropdown  -->
            <div class="d-flex justify-content-between align-items-center mt-8">
                <div>
                    <h1 class="display-4 text-white mb-1">
                        @if (Model.EndDate < Convert.ToDateTime(DateTime.Now.ToString("yyyy-MM-dd")))
                        {
                            <span>0 Ngày</span>
                        }
                        else
                        {
                            @((Model.EndDate - Convert.ToDateTime(DateTime.Now.ToString("yyyy-MM-dd"))).TotalDays + 1) <span>Ngày</span>
                        }
                    </h1>
                    <p class="mb-0 text-white">
                        @Model.EndDate.ToString("dddd"), @Model.EndDate.ToString("dd") Tháng @Model.EndDate.ToString("MM"), @Model.EndDate.ToString("yyyy")
                    </p>
                </div>
                <div>
                    <i class="fe fe-flag fs-1 text-light-primary"></i>
                </div>
            </div>

        </div>

    </div>
    <!-- card  -->
    <div class="card mb-4 ">
        <!-- card body  -->
        <div class="card-body">
            <h4 class=" card-title ">Tiến Độ Tổng Thể </h4>
            <div id="chartTongTheDuAn"></div>
        </div>
    </div>
    <div class="card">
        <!-- Card header -->
        <div class="card-header card-header-height d-flex justify-content-between align-items-center">
            <div>
                <h4 class="mb-0">
                    Hoạt Động Gần Đây
                </h4>
            </div>
            @*<div><a href="#">View All</a></div>*@
        </div>
        <!-- Card body -->
        <div style="max-height: @(heights)px;" class="card-body" data-simplebar>
            <!-- List group -->
            <ul class="list-group list-group-flush list-timeline-activity">
                @if (lichSu.ToList().Count < 1)
                {
                    <li style="margin-top: -10px" class="list-group-item px-0 pt-0 border-0 text-center">
                        Chưa có hoạt động nào.
                    </li>
                }
                else
                {
                    foreach (var item in lichSu)
                    {
                        <li class="list-group-item px-0 pt-0 border-0 pb-6">
                            <div class="row position-relative">
                                <div class="col-auto">
                                    <div class="icon-shape icon-md bg-light-primary text-primary rounded-circle">
                                        <i class="fe fe-clock"></i>
                                    </div>
                                </div>
                                <div class="col ms-n3">
                                    <h4 class="mb-0 h5">@item.Name</h4>
                                    @if (item.ID_Projects != null)
                                    {
                                        <p class="mb-0 text-body"><a>@item.Employees.Name</a> @(item.Contents).</p>
                                    }
                                    else
                                    {
                                        if (item.Contents.IndexOf("|") != -1)
                                        {
                                            <p class="mb-0 text-body">@item.Employees.Name @item.Contents.Split('|')[0] <a data-bs-toggle="tooltip" data-placement="top" title="@item.Tasks.Name" href="#"><u>@item.ID_Task</u></a> @(item.Contents.Split('|')[1]).</p>
                                        }
                                        else if (item.Contents.IndexOf("thêm") != -1)
                                        {
                                            <p class="mb-0 text-body"><a>@item.Employees.Name</a> @item.Contents <a data-bs-toggle="tooltip" data-placement="top" title="@item.Tasks.Name" href="#"><u>@item.ID_Task</u></a> và được giao cho <a>@item.Tasks.Employees.Name.</a></p>
                                        }
                                        else
                                        {
                                            <p class="mb-0 text-body"><a>@item.Employees.Name</a> @item.Contents <a data-bs-toggle="tooltip" data-placement="top" title="@item.Tasks.Name" href="#"><u>@item.ID_Task</u></a>.</p>
                                        }
                                    }
                                </div>
                                <div class="col-auto">
                                    <span class="text-muted fs-6">@item.Date.ToString("HH:mm dd/MM/yy")</span><br />
                                </div>
                            </div>
                        </li>
                    }
                }
            </ul>
        </div>

    </div>
</div>
</div>
</div>
</div>

